load_metrics <- function(path, complexity, mycolname) {
df <- read.csv(path, header = FALSE)
colnames(df) <- mycolname
df$complexity <- complexity
df <- df[df$mode == "Normalized", ]  # keep only normalized rows
df <- select(df,-mode)
return(df)
}
