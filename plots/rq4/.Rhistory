library(dplyr)
library(tidyr)
library(ggplot2)
mycolname<-c("Prec@5","Prec@10","MRR","MAP","kernel","time","seed","mode","numdocs")
load_hybrid <- function(file_path, kernel_name) {
df <- read.csv(file_path, header = TRUE)
colnames(df) <- mycolname
df$kernel[df$kernel == kernel_name] <- paste0("Hybrid-", kernel_name)
df_norm <- df[df$mode == "normalized", ]
return(df_norm)
}
stk_all<-load_hybrid("~/treekernel-jss2025/results/RQ4_v3/STK/metrics_RQ4_hybrid_STK_topk.csv",STK)
stk_all<-load_hybrid("~/treekernel-jss2025/results/RQ4_v3/STK/metrics_RQ4_hybrid_STK_topk.csv","STK")
stk_long <- stk_all %>%
pivot_longer(
cols = c(`Prec@5`, `Prec@10`, MRR, MAP),
names_to = "metric",
values_to = "value"
)
ggplot(stk_long, aes(x = factor(numdocs), y = value)) +
geom_boxplot(outlier.size = 1) +
facet_wrap(~ metric, scales = "free_y") +
labs(x = "Number of Documents", y = "Score") +
theme_bw()
